;THIS PROGRAM CALCULATES IF A GIVEN NUMBER IS PRIME OR NOT


JMP 0x0002  ;SET NUMBER TO TEST
SKP m,,,
MST 0x01 12

LDB 0x01    ;INITIAL VARIABLES
LIC 1
LDD 0x0001

MOV A B     ;OUTER FACTOR TEST LOOP
SUB D C
CMP D C
BZE 0x000E

SUB A D     ;INNER MODULUS LOOP
BZE 0x000E
BNE 0x0006
BRA 0x000A

MST 0x01 0  ;NOT PRIME END
HLT

MST 0x01 1  ;PRIME END
HLT         ;result in: 0x0001

